<region>
key=$HIHAT_PEDAL_KEY
<region>
key=$HIHAT_CLOSED_BOW_KEY
<region>
key=$HIHAT_CLOSED_EDGE_KEY


// open hihat choked by half, pedal, and closed
<group>
