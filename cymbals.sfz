// Hihat choke rules
<master>
polyphony=3

// hihat splash choked by open, half, pedal, and closed
<group>
sample=*silence
loop_mode=loop_continuous
off_mode=normal
ampeg_attack=0
ampeg_decay=0
ampeg_sustain=0
ampeg_release=0
group=5
off_by=5
<region>
key=$HIHAT_OPEN_BOW_KEY
<region>
key=$HIHAT_OPEN_EDGE_KEY
<region>
key=$HIHAT_HALF_BOW_KEY_1
<region>
